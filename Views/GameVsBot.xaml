    <Canvas>
        <Canvas.Background>
            <ImageBrush ImageSource="pack://application:,,,/Monopoly;component/Resources/option.jpg"/>
        </Canvas.Background>
        <!--<Rectangle Height="100"
                   Width="100"
                   Canvas.Top="{Binding map[0].TopMap}"
                   Canvas.Left="{Binding map[0].LeftMap}" Fill="Black"/>-->
        <Border Height="100"
                   Width="100"
                   Canvas.Top="{Binding map[0].TopMap}"
                   Canvas.Left="{Binding map[0].LeftMap}"
                BorderBrush="Black"
                BorderThickness="1">
            <TextBlock Text="Begin" FontSize="20" FontWeight="Bold"
                       FontFamily="Ink Free"
                       VerticalAlignment="Bottom"
                       HorizontalAlignment="Center"/>
        </Border>
        
        <!--<Rectangle Height="100"
                   Width="70"
