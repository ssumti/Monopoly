                Width="70"
                Canvas.Top="{Binding map[1].TopMap}"
                Canvas.Left="{Binding map[1].LeftMap}"
                BorderBrush="Black"
                BorderThickness="1">
            <StackPanel VerticalAlignment="Bottom"
                        HorizontalAlignment="Center">
                <TextBlock Text="{Binding MapTile[1].realEstate.Name}" FontSize="12.5" FontWeight="Bold"
                       FontFamily="Ink Free"/>
                <StackPanel Orientation="Horizontal">
                    <TextBlock Text="{Binding MapTile[1].realEstate.BuyPrice}" FontSize="12.5" FontWeight="Bold"
                       FontFamily="Ink Free"/>
                    <TextBlock Text="$" FontSize="12.5" FontWeight="Bold"
                       FontFamily="Ink Free"/>
                    <TextBlock Text="(" FontSize="12.5" FontWeight="Bold"
                       FontFamily="Ink Free"/>
                    <TextBlock Text="{Binding MapTile[1].Owner}" FontSize="12.5" FontWeight="Bold"
                       FontFamily="Ink Free"/>
                    <TextBlock Text=")" FontSize="12.5" FontWeight="Bold"
                       FontFamily="Ink Free"/>
                </StackPanel>
            </StackPanel>
        </Border>
        
